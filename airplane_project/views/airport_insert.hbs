<h1>airport 데이터 삽입</h1>

<form name="airport" method="post" action="/">
    <div>
        <label for="airport_code">Airport_Code</label>
        <input id="airport_code" name="airport_code" type="text" required placeholder="Airport_Code">
    </div>
    <div>
        <label for="airport_name">Airport_Name</label>
        <input id="airport_name" name="airport_name" type="text" required placeholder="Airport_Name">
    </div>
    <div>
        <label for="city">City</label>
        <input id="city" name="city" type="text" required placeholder="City">
    </div>
    <div>
        <label for="state">State</label>
        <input id="state" name="state" type="text" placeholder="State">
    </div>
    <br>
    <div>
        <input type="submit" value="삽입" formaction="/admin/airport/insert">
    </div>
</form>

